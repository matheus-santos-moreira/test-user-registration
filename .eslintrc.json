{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-undef":"off",
      "class-methods-use-this":"off",
      "prettier/prettier": "error",
      "no-unused-vars": ["error", {
        "argsIgnorePattern": "_"
      }]
    }
}
